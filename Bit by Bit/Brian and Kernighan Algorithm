//Count set bits by Brian and Kernighan Algorithm
//Author : Infra

#include <bits/stdc++.h>
using namespace std;

int Brutforce(int n) //o(logn) approach
{
    int c=0;
    while(n)
    {
        c+=n&1;
        n>>=1;
    }
    return c;
}

int BrianKerninghan(int n) //using toggling effect if right most bit is 0
{
    int c=0;
    while(n)
    {
        c++;
        n&=(n-1);
    }
    return c;
}

int main()
{
    int n;
    cin>>n;
    cout<<Brutforce(n)<<" "<<BrianKerninghan(n);
}
